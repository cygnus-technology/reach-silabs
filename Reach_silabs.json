{
    "name": "Thunderboard",
    "manufacturer": "Silicon Labs",
    "description": "A demo of Reach features",
    "services":
    {
        "parameterRepositoryService": {
            "parameters": [
                {
                    "name": "User Device Name",
                    "description": "The advertised BLE name",
                    "access": "Read/Write",
                    "storageLocation": "NVM",
                    "dataType": "string",
                    "maxSize": 29
                },
                {
                    "name": "Uptime",
                    "access": "Read",
                    "storageLocation": "RAM",
                    "dataType": "int64",
                    "units": "milliseconds"
                },
                {
                    "name": "Button Pressed",
                    "access": "Read",
                    "storageLocation": "RAM",
                    "dataType": "boolean",
                    "labelName": "Identify Switch"
                },
                {
                    "name": "Identify LED",
                    "access": "Read",
                    "storageLocation": "RAM",
                    "dataType": "boolean",
                    "labelName": "Identify LED",
                    "defaultNotifications": {"minInterval": 302, "minDelta": 1}
                },
                {
                    "name": "Color Demo RGB State",
                    "description": "RGB representation of colors",
                    "access": "Read/Write",
                    "storageLocation": "RAM",
                    "dataType": "bitfield",
                    "bitsAvailable": 3,
                    "labelName": "RGB State"
                },
                {
                    "name": "Color Demo Enumeration",
                    "description": "Human-readable RGB translation",
                    "access": "Read/Write",
                    "storageLocation": "RAM",
                    "dataType": "enumeration",
                    "rangeMin": 0,
                    "rangeMax": 7,
                    "labelName": "RGB State Translation"
                },
                {
                    "name": "Identify",
                    "description": "Turn on to blink the yellow LED",
                    "access": "Read/Write",
                    "storageLocation": "RAM",
                    "dataType": "boolean",
                    "defaultValue": false,
                    "defaultNotifications": {"minInterval": 501, "minDelta": 1}
                },
                {
                    "name": "Identify Interval",
                    "access": "Read/Write",
                    "storageLocation": "NVM",
                    "dataType": "float32",
                    "rangeMin": 0.01,
                    "rangeMax": 60,
                    "defaultValue": 1
                },

                {
                    "name": "Relative Humidity",
                    "description": "Read from Thunderboard Sensor",
                    "access": "Read",
                    "units": "%",
                    "storageLocation": "RAM",
                    "dataType": "float32",
                    "rangeMin": 0.01,
                    "rangeMax": 100,
                    "precision": 1,
                    "defaultNotifications": {"minInterval": 2010, "maxInterval": 60000, "minDelta": 1}
                },
                {
                    "name": "Temperature",
                    "description": "Read from Thunderboard Sensor",
                    "access": "Read",
                    "storageLocation": "RAM",
                    "dataType": "float32",
                    "rangeMin": -57,
                    "rangeMax": 99,
                    "precision": 1,
                    "units": "\u00B0 C",
                    "defaultNotifications": {"minInterval": 2020, "maxInterval": 60000, "minDelta": 0.2}
                },
                {
                    "name": "Light Level",
                    "description": "Read from Thunderboard Sensor",
                    "access": "Read",
                    "storageLocation": "RAM",
                    "dataType": "float32",
                    "rangeMin": 0,
                    "rangeMax": 1000,
                    "precision": 1,
                    "units": "lux",
                    "defaultNotifications": {"minInterval": 1020, "maxInterval": 60000, "minDelta": 20}
                },
                {
                    "name": "UV Index",
                    "description": "Read from Thunderboard Sensor",
                    "access": "Read",
                    "storageLocation": "RAM",
                    "dataType": "float32",
                    "rangeMin": 0,
                    "rangeMax": 100,
                    "precision": 1,
                    "units": "W/sq meter",
                    "defaultNotifications": {"minInterval": 3031, "maxInterval": 60000, "minDelta": 1}
                },
                {
                    "name": "Magnetic Field Strength",
                    "description": "Read from Thunderboard Sensor",
                    "access": "Read",
                    "storageLocation": "RAM",
                    "dataType": "float32",
                    "rangeMin": 0,
                    "rangeMax": 5000,
                    "precision": 1,
                    "units": "millitesla",
                    "defaultNotifications": {"minInterval": 3019, "maxInterval": 60000, "minDelta": 0.2}
                },
                {
                    "name": "Acceleration (X axis)",
                    "description": "Read from Thunderboard Sensor",
                    "access": "Read",
                    "storageLocation": "RAM",
                    "dataType": "float32",
                    "rangeMin": -50,
                    "rangeMax": 50,
                    "precision": 2,
                    "units": "m/s",
                    "defaultNotifications": {"minInterval": 2037, "minDelta": 0.2}
                },
                {
                    "name": "Acceleration (Y axis)",
                    "description": "Read from Thunderboard Sensor",
                    "access": "Read",
                    "storageLocation": "RAM",
                    "dataType": "float32",
                    "rangeMin": -50,
                    "rangeMax": 50,
                    "precision": 2,
                    "units": "m/s",
                    "defaultNotifications": {"minInterval": 2047, "minDelta": 0.2}
                },
                {
                    "name": "Acceleration (Z axis)",
                    "description": "Read from Thunderboard Sensor",
                    "access": "Read",
                    "storageLocation": "RAM",
                    "dataType": "float32",
                    "rangeMin": -50,
                    "rangeMax": 50,
                    "precision": 2,
                    "units": "m/s",
                    "defaultNotifications": {"minInterval": 2057, "minDelta": 0.2}
                },

                {
                    "name": "Timezone Enabled",
                    "access": "Read/Write",
                    "storageLocation": "NVM",
                    "dataType": "boolean",
                    "defaultValue": true
                },
                {
                    "name": "Timezone Offset",
                    "access": "Read/Write",
                    "storageLocation": "NVM",
                    "dataType": "int32",
                    "units": "seconds",
                    "defaultValue": 0,
                    "rangeMin": -43200,
                    "rangeMax": 43200
                },
                {
                    "name": "BT Device Address",
                    "access": "Read",
                    "storageLocation": "RAM",
                    "dataType": "bytearray",
                    "maxSize": 6
                }

            ],
            "extendedLabels": [
                {
                    "name": "Identify Switch",
                    "dataType": "boolean",
                    "falseLabel": "Not Identifying",
                    "trueLabel": "Identifying"
                },
                {
                    "name": "Identify LED",
                    "dataType": "boolean",
                    "falseLabel": "Off",
                    "trueLabel": "Illuminated"
                },
                {
                    "name": "timezone",
                    "dataType": "boolean",
                    "falseLabel": "Non-Timezoned",
                    "trueLabel": "Timezoned"
                },
                {
                    "name": "RGB State",
                    "dataType": "bitfield",
                    "bitIndices": [
                        {
                            "label": "Red"
                        },
                        {
                            "label": "Green"
                        },
                        {
                            "label": "Blue"
                        }
                    ]
                },
                {
                    "name": "RGB State Translation",
                    "dataType": "enumeration",
                    "enumValues": [
                        {
                            "label": "Off"
                        },
                        {
                            "label": "Red"
                        },
                        {
                            "label": "Green"
                        },
                        {
                            "label": "Yellow"
                        },
                        {
                            "label": "Blue"
                        },
                        {
                            "label": "Magenta"
                        },
                        {
                            "label": "Cyan"
                        },
                        {
                            "label": "White"
                        }
                    ]
                }
            ]
        },
        "fileService": {
            "files": [
                {
                    "name": "io.txt",
                    "maxSize": 2048,
                    "access": "Read/Write",
                    "storageLocation": "NVM",
                    "requireChecksum": false
                },
                {
                    "name": "cygnus-reach-logo.png",
                    "maxSize": 17900,
                    "access": "Read",
                    "storageLocation": "NVM",
                    "requireChecksum": false
                },
                {
                    "name": "dev_null",
                    "maxSize": 100000,
                    "access": "Read/Write",
                    "storageLocation": "RAM",
                    "requireChecksum": false
                }
            ]
        },
        "commandService": {
            "commands": [
                {
                    "name": "Reset Defaults",
                    "description": "Restore factory values."
                },
                {
                    "name": "Click for Wisdom",
                    "description": "Press it and find out"
                },
                {
                    "name": "No logging",
                    "description": "lm 0"
                },
                {
                    "name": "Much logging",
                    "description": "lm 1c7"
                },
                {
                    "name": "Notifications On",
                    "description": "Enable notifications on changes"
                },
                {
                    "name": "Notifications Off",
                    "description": "disable all notifications"
                },
                {
                    "name": "Remote CLI On",
                    "description": "To interact remotely"
                },
                {
                    "name": "Remote CLI Off",
                    "description": "Avoids heavy communication load"
                }
            ]
        },
        "cliService": {
            "commands": [
                {
                    "string": "ver",
                    "description": "Print versions"
                },
                {
                    "string": "/",
                    "description": "Display status"
                },
                {
                    "string": "lm",
                    "argDescription": "(<new log mask>)",
                    "description": "Print current log mask, or set a new log mask"
                }
            ]
        },
        "timeService": {},
		"wifiService": {"maxAccessPoints": 4},
        "streamService": {
            "streams":  [
                {
                    "name": "thing1-R",
                    "access": "Read",
                    "description": "Read this stream"
                },
                {
                    "name": "thing2-W",
                    "access": "Write",
                    "description": "Write this stream"
                },
                {
                    "name": "thing3-R",
                    "access": "Read",
                    "description": "Read this stream 3"
                },
                {
                    "name": "thing4-W",
                    "access": "Write",
                    "description": "Write this stream 4"
                },
                {
                    "name": "thing5-R",
                    "access": "Read",
                    "description": "Read this stream 5"
                },
                {
                    "name": "thing6-W",
                    "access": "Write",
                    "description": "Write this stream 6"
                }
            ]

        }
    }
}