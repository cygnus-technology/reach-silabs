{
	"name": "Thunderboard",
	"manufacturer": "Silicon Labs",
	"description": "A demo of Reach features",
	"services":
	{
		"parameterRepositoryService": {
			"features": {
				"descriptions": true,
				"discovery": true,
				"storage": true
			},
			"parameters": [
				{
					"name": "User Device Name",
					"description": "The advertised BLE name",
					"access": "Read/Write",
					"storageLocation": "NVM",
					"dataType": "string",
					"maxSize": 29
				},
				{
					"name": "Uptime",
					"access": "Read",
					"storageLocation": "RAM",
					"dataType": "int64",
					"units": "milliseconds"
				},
				{
					"name": "Button Pressed",
					"access": "Read",
					"storageLocation": "RAM",
					"dataType": "boolean",
                    "labelName": "Identify Switch"
				},
				{
					"name": "Identify LED",
					"access": "Read",
					"storageLocation": "RAM",
					"dataType": "boolean",
					"labelName": "Identify LED",
                    "minimum_notification_period": 302,
                    "maximum_notification_period": 0,
                    "minimum_delta": 1
				},
				{
					"name": "RGB LED State",
					"description": "A demo of a bitfield",
					"access": "Read/Write",
					"storageLocation": "RAM",
					"dataType": "bitfield",
					"bitsAvailable": 3,
					"labelName": "BittyBit"
				},
				{
					"name": "Command Line Color",
					"description": "Reset on disconnection",
					"access": "Read/Write",
					"storageLocation": "RAM",
					"dataType": "enumeration",
					"rangeMin": 0,
					"rangeMax": 7,
					"labelName": "CLI Color"
				},
				{
					"name": "Identify",
					"description": "Turn on to blink the yellow LED",
					"access": "Read/Write",
					"storageLocation": "RAM",
					"dataType": "boolean",
					"defaultValue": false,
                    "minimum_notification_period": 501,
                    "maximum_notification_period": 0,
                    "minimum_delta": 1
				},
				{
					"name": "Identify Interval",
					"access": "Read/Write",
					"storageLocation": "NVM",
					"dataType": "float32",
					"rangeMin": 0.01,
					"rangeMax": 60,
					"defaultValue": 1
				},

				{
					"name": "Relative Humidity",
                    "description": "Read from Thunderboard Sensor",
					"access": "Read",
                    "units": "%",
					"storageLocation": "RAM",
					"dataType": "float32",
					"rangeMin": 0.01,
					"rangeMax": 100,
                    "precision": 1,
                    "minimum_notification_period": 2010,
                    "maximum_notification_period": 60000,
                    "minimum_delta": 1.0
				},
                {
                    "name": "Temperature",
                    "description": "Read from Thunderboard Sensor",
                    "access": "Read",
                    "storageLocation": "RAM",
                    "dataType": "float32",
                    "rangeMin": -57,
                    "rangeMax": 99,
                    "precision": 1,
                    "units": "\u00B0 C",
                    "minimum_notification_period": 2020,
                    "maximum_notification_period": 60000,
                    "minimum_delta": 0.2
                },
                {
                    "name": "Light Level",
                    "description": "Read from Thunderboard Sensor",
                    "access": "Read",
                    "storageLocation": "RAM",
                    "dataType": "float32",
                    "rangeMin": 0,
                    "rangeMax": 1000,
                    "precision": 1,
                    "units": "lux",
                    "minimum_notification_period": 1020,
                    "maximum_notification_period": 60000,
                    "minimum_delta": 20.0
                },
                {
                    "name": "UV Index",
                    "description": "Read from Thunderboard Sensor",
                    "access": "Read",
                    "storageLocation": "RAM",
                    "dataType": "float32",
                    "rangeMin": 0,
                    "rangeMax": 100,
                    "precision": 1,
                    "units": "W/sq meter",
                    "minimum_notification_period": 3031,
                    "maximum_notification_period": 60000,
                    "minimum_delta": 1.0
                },
                {
                    "name": "Magnetic Field Strength",
                    "description": "Read from Thunderboard Sensor",
                    "access": "Read",
                    "storageLocation": "RAM",
                    "dataType": "float32",
                    "rangeMin": 0,
                    "rangeMax": 5000,
                    "precision": 1,
                    "units": "millitesla",
                    "minimum_notification_period": 3019,
                    "maximum_notification_period": 60000,
                    "minimum_delta": 0.2
                },
                {
                    "name": "Acceleration (X axis)",
                    "description": "Read from Thunderboard Sensor",
                    "access": "Read",
                    "storageLocation": "RAM",
                    "dataType": "float32",
                    "rangeMin": -50,
                    "rangeMax": 50,
                    "precision": 2,
                    "units": "m/s",
                    "minimum_notification_period": 2037,
                    "maximum_notification_period": 0,
                    "minimum_delta": 0.2
                },
                {
                    "name": "Acceleration (Y axis)",
                    "description": "Read from Thunderboard Sensor",
                    "access": "Read",
                    "storageLocation": "RAM",
                    "dataType": "float32",
                    "rangeMin": -50,
                    "rangeMax": 50,
                    "precision": 2,
                    "units": "m/s",
                    "minimum_notification_period": 2047,
                    "maximum_notification_period": 0,
                    "minimum_delta": 0.2
                },
                {
                    "name": "Acceleration (Z axis)",
                    "description": "Read from Thunderboard Sensor",
                    "access": "Read",
                    "storageLocation": "RAM",
                    "dataType": "float32",
                    "rangeMin": -50,
                    "rangeMax": 50,
                    "precision": 2,
                    "units": "m/s",
                    "minimum_notification_period": 2057,
                    "maximum_notification_period": 0,
                    "minimum_delta": 0.2
                },

                {
                    "name": "Timezone Enabled",
                    "access": "Read/Write",
                    "storageLocation": "NVM",
                    "dataType": "boolean",
                    "defaultValue": true
                },
                {
                    "name": "Timezone Offset",
                    "access": "Read/Write",
                    "storageLocation": "NVM",
                    "dataType": "int32",
                    "units": "seconds",
                    "defaultValue": 0,
                    "rangeMin": -43200,
                    "rangeMax": 43200
                },
                {
                    "name": "BT Device Address",
                    "access": "Read",
                    "storageLocation": "RAM",
                    "dataType": "bytearray",
                    "maxSize": 6
                }

			],
			"extendedLabels": [
				{
					"name": "Identify Switch",
					"dataType": "boolean",
					"falseLabel": "Not Identifying",
					"trueLabel": "Identifying"
				},
                {
                    "name": "Identify LED",
                    "dataType": "boolean",
                    "falseLabel": "Off",
                    "trueLabel": "Illuminated"
                },
                {
                    "name": "timezone",
                    "dataType": "boolean",
                    "falseLabel": "Non-Timezoned",
                    "trueLabel": "Timezoned"
                },
				{
					"name": "BittyBit",
					"dataType": "bitfield",
					"bitIndices": [
						{
							"label": "Red"
						},
						{
							"label": "Green"
						},
						{
							"label": "Blue"
						}
					]
				},
				{
					"name": "CLI Color",
					"dataType": "enumeration",
					"enumValues": [
						{
							"label": "Off"
						},
						{
							"label": "Red"
						},
						{
							"label": "Green"
						},
						{
							"label": "Yellow"
						},
						{
							"label": "Blue"
						},
						{
							"label": "Magenta"
						},
						{
							"label": "Cyan"
						},
						{
							"label": "White"
						}
					]
				}
			]
		},
		"fileService": {
			"features": {
				"descriptions": true,
				"discovery": true
			},
			"files": [
				{
					"name": "io.txt",
					"maxSize": 2048,
					"access": "Read/Write",
					"storageLocation": "NVM"
				},
				{
					"name": "cygnus-reach-logo.png",
					"maxSize": 17900,
					"access": "Read",
					"storageLocation": "NVM"
				},
				{
					"name": "dev_null",
					"maxSize": 100000,
					"access": "Read/Write",
					"storageLocation": "RAM"
				}
			]
		},
		"commandService": {
			"features": {
				"descriptions": true,
				"discovery": true
			},
			"commands": [
				{
					"name": "Reset Defaults",
					"description": "Restore factory values."
				},
				{
					"name": "Click for Wisdom",
					"description": "Press it and find out"
				},
				{
					"name": "No logging",
					"description": "lm 0"
				},
                {
                    "name": "Much logging",
                    "description": "lm 1c7"
                },
                {
                    "name": "Notifications On",
                    "description": "Enable notifications on changes"
                },
                {
                    "name": "Notifications Off",
                    "description": "disable all notifications"
                },
                {
                    "name": "Remote CLI On",
                    "description": "To interact remotely"
                },
                {
                    "name": "Remote CLI Off",
                    "description": "Avoids heavy communication load"
                }
			]
		},
		"cliService": {
			"features": {
				"ble": true,
				"uart": true
			},
			"commands": [
				{
					"string": "ver",
					"description": "Print versions"
				},
				{
					"string": "/",
					"description": "Display status"
				},
				{
					"string": "lm",
					"description": "Get ('lm') or set ('lm <hex value>') the current Reach log mask"
				}
			]
		},
        "streamService": {
            "streams": [
                {
                    "name": "from device",
                    "access": "read",
                    "description": "counting up for a test"
                },
                {
                    "name": "to device",
                    "access": "write",
                    "description": "throw away received data"
                }
            ]
        },
        "WiFService": {
            "AP count": 4
        }
	}
}